{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Supabase": {
    "Url": "YOUR_SUPABASE_URL", // e.g., https://your-project-id.supabase.co
    "PublicKey": "YOUR_SUPABASE_PUBLIC_KEY", // For JWT validation (anon key if used for JWT signing, or dedicated public key)
    "ServiceKey": "YOUR_SUPABASE_SERVICE_KEY", // If needed for admin operations (use with caution)
    "JwtSecret": "YOUR_SUPABASE_JWT_SECRET" // If using Supabase JWT directly and it's symmetric; otherwise, use PublicKey for asymmetric
  },
  "ConnectionStrings": {
    "SupabaseDB": "Host=your-supabase-db-host;Port=5432;Database=postgres;Username=postgres;Password=your-db-password"
    // e.g., Host=db.your-project-id.supabase.co
  }
}
